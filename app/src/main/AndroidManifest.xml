<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pascal_pc.tasklist">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_img"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_img"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TaskPagerActivity"></activity>
        <activity android:name=".TaskDetailActivity" />
        <activity android:name=".CreateNewTaskActivity" />
        <activity android:name=".LogInActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ResgisterActivity"></activity>
    </application>

</manifest>